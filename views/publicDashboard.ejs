
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" type="text/css" href="css/publicdash.css">
    <title></title>
</head>
<body>
    <nav class="navbar">
        <h1>Blog</h1>
    </nav>
    <section class="dash">
        <% if(user != 'undefine') {%>
            <% user.forEach(ele => { %>
                <div class="upper">
                        <p id="name">name: <%= ele.name%></p>
                        <p id="title">title: <%= ele.title%></p>
                        <p id="category"><%= ele.category%></p>
                        <a href="/id/<%=ele._id%>">-></a>
                </div>
            <%});%> 
        <% } %>   
    </section>
</body>
</html>
